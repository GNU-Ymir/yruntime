 echo | gyc -E -Wp,-v - &> files
Y_INCLUDE_PATH=$( grep -A1 "<...>" files | tail -n -1)/ymir/
 echo $Y_INCLUDE_PATH
 rm -r $Y_INCLUDE_PATH
 mkdir -p $Y_INCLUDE_PATH/core/
 mkdir -p $Y_INCLUDE_PATH/std/
 mkdir -p $Y_INCLUDE_PATH/etc/

 cp -r core/ $Y_INCLUDE_PATH
 cp -r std/ $Y_INCLUDE_PATH
 cp -r etc/ $Y_INCLUDE_PATH
 rm files

