cores=$(ls midgard/core | grep [.]yr$)
std=$(ls midgard/std | grep [.]yr$)
collection=$(ls midgard/std/collection | grep [.]yr$)
etc=$(ls midgard/etc | grep [.]yr$)
etc_c=$(ls midgard/etc/c | grep [.]yr$)
etc_run=$(ls midgard/etc/runtime | grep [.]yr$)

mkdir -p /usr/include/ymir
cd midgard/
find * -regex '.*\.\(yr\)$' | cpio -updm /usr/include/ymir/
