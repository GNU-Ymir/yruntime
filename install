rm -rf /usr/include/ymir
mkdir -p /usr/include/ymir
cd midgard/
find * -regex '.*\.\(yr\)$' | cpio -updm /usr/include/ymir/1.0

cd ..
rm -rf /usr/libexec/gcc/x86_64-linux-gnu/13/include/ymir/1.0
mkdir -p /usr/libexec/gcc/x86_64-linux-gnu/13/include/ymir/1.0
cd midgard/
find * -regex '.*\.\(yr\)$' | cpio -updm /usr/libexec/gcc/x86_64-linux-gnu/13/include/ymir/1.0
