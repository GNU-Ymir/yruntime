in empty;

use std::{stream, conv};

/**
 * Configuration encoding of nothing.
 * @example:
 * ```
 * let node : &Config = copy None ();
 *
 * match node {
 *    None () => {}
 * }
 * ```
 */
@final
pub class None over Config {
    pub self () {}

    impl Streamable {
        pub over toStream (self, dmut stream : &StringStream)-> void {
            stream:.write ("None ()"s8);
        }
    }
}
