in vec;

use std::io;
use std::collection::vec;

mod utils {

    class X {
        let mut x = 0;
        pub self () {}
    }


}

__test {
    let dmut a = copy Vec!{i32} ();
    a:.push (1);
    a:.push (2);

    assert (a [0] == 1);
    assert (a [1] == 2);
}

__test {
    let dmut a = copy Vec!{i32} ();
    for i in 0 ... 5 {
        a:.push (i);
    }

    let z = copy [i for i in a];
    assert (z == [0, 1, 2, 3, 4, 5]);
}
