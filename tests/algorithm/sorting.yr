in sorting;

use std::io;
use std::algorithm::sorting;


__test {
    let dmut a = copy [4, 65, 2, -31, 0, 99, 2, 83, 782, 1];
    let b = (alias a).sort ();

    assert (b == [-31, 0, 1, 2, 2, 4, 65, 83, 99, 782]);
    assert (a == b);
}

__test {
    let dmut a = copy [4, 65, 2, -31, 0, 99, 2, 83, 782, 1];
    let b = (alias a).sort (|x, y| { x > y });

    assert (b == [782, 99, 83, 65, 4, 2, 2, 1, 0, -31]);
    assert (a == b);
}
