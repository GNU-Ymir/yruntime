in comparison;

use std::io;
use std::conv;
use std::algorithm::comparison;

__test {
    assert (min (1, 2) == 1);
    assert (min (9827, 8161) == 8161);
    assert (min (1u16, 2u16) == 1);
    assert (min (9827u64, 8161u64) == 8161);


    assert (min (2, 3, 1, 9, 4) == 1);
    assert (max (2, 3, 1, 9, 4) == 9);
}
