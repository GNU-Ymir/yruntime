in algorithm;

use std::algorithm::searching;

mod comparison;
mod iteration;
mod searching;

__test {
    let a = copy [1, 2, 3];
    assert (a.all!{|x| => x <= 3}());
    assert (a.all (|x| => x <= 3));

    let z = 3;
    assert (a.all (copy |x| => x <= z));
}


__test {
    let a = copy [1, 2, 3];
    assert (a.any!{|x| => x == 3}());
    assert (a.any (|x| => x == 3));

    let z = 3;
    assert (a.any (copy |x| => x == z));
}
