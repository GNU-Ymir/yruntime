in box;

use std::{box, io};

__test {
    let dmut a = copy Box!{i32} (1);
    a.value = 899;
    assert (a.value == 899);
}


class X {
    pub let mut x : i32 = 1;
    pub self () {}
}

__test {
    let dmut a = copy Box!{dmut &X} (copy X ());
    a.value.x = 899;
    assert (a.value.x == 899);
}
